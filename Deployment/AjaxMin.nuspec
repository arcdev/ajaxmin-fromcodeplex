        <file src="AjaxMinTask\bin\$Configuration$\AjaxMinLibrary.dll" target="tools/net40"/>
        <file src="AjaxMinTask\bin\$Configuration$\AjaxMinLibrary.pdb" target="tools/net40"/>
        <file src="**\*.cs" target="src"/>
